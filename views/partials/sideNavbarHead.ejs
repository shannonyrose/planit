<div class="container-fluid">
    <div class="row">
        <div class="col-1" id="sideNav">
            <div class="row">
                <a class="navbar-brand sideNavBrand" href="#">planit</a>
            </div>
            <div class="container sideNav-itemsContainer">
                <ul class="sideNav-items">
                    <div class="row sideNav-itemContainer profilePicture">
                        <li><img id="profilePicture" src="/imgs/toshi-lySzv_cqxH8-unsplash.jpg" alt=""></li>
                    </div>
                    <div class="row sideNav-itemContainer">
                        <li class="sideNav-listItem">
                            <div class="sideNav-item-wrapper">
                                <span class="icon-wrapper">
                                    <i class="bi-file-person"></i>
                                </span>
                                <a class="sideNav-item" href="/todo">Dashboard</a>
                            </div>
                        </li>
                    </div>
                    <div class="row sideNav-itemContainer">
                        <li class="sideNav-listItem">
                            <div class="sideNav-item-wrapper">
                                <span class="icon-wrapper">
                                    <i class="bi-check2-square"></i>
                                </span>
                                <a class="sideNav-item" href="/projects">Projects</a>
                            </div>
                        </li>
                    </div>
                    <div class="row sideNav-itemContainer">
                        <ul class="sideNav-projectList">
                            <% for(let project of foundProject) { %>
                                <a class="projectTitleLink" href="/projects/<%= project._id %>">
                                    <li class="projectTitle">
                                        <i class="projectListItemIcon bi bi-square-fill"></i>
                                        <%= project.title %>
                                    </li>
                                </a>
                                <% } %>
                        </ul>
                    </div>
                </ul>
            </div>
        </div>
        <div class="col-1"></div>
        <div class="col-11" id="mainContent">