<%- include('partials/header') %>

    <%- include('partials/footer') %>
        <div class="info container-fluid">
            <div class="row">
                <div class="col" id="leftLogIn">
                    <%- include('partials/navbar') %>
                </div>
                <div class="col" id="rightLogIn">
                    <div class="container" id="loginInformation">
                            <form action="">
                                <div class="row"><label class = "loginInfoLabel" for="username">Username</label></div>
                                <div class="row"><input class="signIn" name="username" type="text"></div>
                                <div class="d-flex flex-row-reverse"><a class = "forgotLoginInfo" href="">Forgot username?</a></div>
                                <div class="row mt-3"><label class = "loginInfoLabel" for="password">Password</label></div>
                                <div class="row"><input class="signIn" name="password" type="text"></div>
                                <div class="d-flex flex-row-reverse"><a class = "forgotLoginInfo" href="">Forgot password?</a></div>
                                <div class="row buttons">
                                        <div class="loginButtons col-6">
                                            <button class="submit" name="signUp">Sign Up</button>
                                        </div>
                                        <div class="loginButtons col-6">
                                            <button class="submit" name="login">Login</button>
                                        </div>
                                </div>
                            </form>
                    </div>
                </div>
            </div>
        </div>
        </div>